extension=apc.so
apc.rfc1867=<%= node['php']['apc']['apc.rfc1867'] %>
apc.enabled=<%= node['php']['apc']['apc.enabled'] %>
apc.shm_segments=<%= node['php']['apc']['apc.shm_segments'] %>
apc.ttl=<%= node['php']['apc']['apc.ttl'] %>
apc.user_ttl=<%= node['php']['apc']['apc.user_ttl'] %>
apc.num_files_hint=<%= node['php']['apc']['apc.num_files_hint'] %>
; apc.mmap_file_mask=/tmp/apc.XXXXXX
apc.enable_cli=<%= node['php']['apc']['apc.enable_cli'] %>
